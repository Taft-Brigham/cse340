<h1><%- title %></h1>
    <% if (locals.notice) { %>
      <div class="flash-message"><%- notice %></div>
    <% } %>
    <% if (errors && errors.length > 0) { %>
      <ul>
        <% errors.forEach(function(error) { %>
          <li><%- error.msg %></li>
        <% }) %>
      </ul>
    <% } %>
    <p>You're logged in</p>